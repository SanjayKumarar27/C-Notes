@model List<WebMVC.Models.emp>

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}


<table>
    <tr>
        <th>Id</th>
         <th>Name</th>
        <th>Salary</th>
    </tr>
        @foreach(var emp in Model)
        {
    <tr>
                 <td>@emp.Id</td>
                 <td>@emp.Name</td>
                <td>@emp.Sal</td>
    </tr>
        }
</table>
